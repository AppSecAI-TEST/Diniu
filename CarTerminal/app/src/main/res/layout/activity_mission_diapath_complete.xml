<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/bg_grey"
              android:orientation="vertical">

    <!--开始完成时间-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y98"
        android:layout_marginTop="@dimen/y7"
        android:background="@android:color/white"
        android:gravity="center"
        android:orientation="vertical"
        android:paddingLeft="@dimen/x12"
        android:paddingRight="@dimen/x12">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/y17"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/x8"
                android:layout_height="@dimen/x8"
                android:layout_marginRight="@dimen/x7"
                android:src="@drawable/drawable_blue_dot"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/x79"
                android:text="开始时间"
                android:textColor="#5c5c5c"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/missionDisComAct_tv_starTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xxx"
                android:textColor="#2f2f2f"
                android:textSize="15sp"/>

        </LinearLayout>

        <include layout="@layout/common_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y17"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/x8"
                android:layout_height="@dimen/x8"
                android:layout_marginRight="@dimen/x7"
                android:src="@drawable/drawable_red_dot"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/x79"
                android:text="完成时间"
                android:textColor="#5c5c5c"
                android:textSize="14sp"/>

            <TextView
                android:id="@+id/missionDisComAct_tv_endTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xxx"
                android:textColor="#2f2f2f"
                android:textSize="15sp"/>

        </LinearLayout>

    </LinearLayout>

    <!--任务-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y98"
        android:layout_marginTop="@dimen/y7"
        android:background="@android:color/white"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:paddingLeft="@dimen/x12"
        android:paddingRight="@dimen/x12">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/y10"
            android:layout_marginRight="@dimen/x79"
            android:drawableLeft="@mipmap/mission"
            android:drawablePadding="@dimen/x7"
            android:text="任务"
            android:textColor="#5c5c5c"
            android:textSize="14sp"/>

        <include layout="@layout/common_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y10"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="将"
                android:textColor="#2c2c2c"
                android:textSize="15sp"/>

            <TextView
                android:id="@+id/missionDisComAct_tv_plateNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xxx"
                android:textColor="#edb643"
                android:textSize="15sp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="还车至"
                android:textColor="#2c2c2c"
                android:textSize="15sp"/>

            <TextView
                android:id="@+id/missionDisComAct_tv_endName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xxx"
                android:textColor="#edb643"
                android:textSize="15sp"/>

        </LinearLayout>

    </LinearLayout>

    <!--车型-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y98"
        android:layout_marginTop="@dimen/y7"
        android:background="@android:color/white"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:paddingLeft="@dimen/x12"
        android:paddingRight="@dimen/x12">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/y10"
            android:layout_marginRight="@dimen/x79"
            android:drawableLeft="@mipmap/car_2"
            android:drawablePadding="@dimen/x7"
            android:text="车型"
            android:textColor="#5c5c5c"
            android:textSize="14sp"/>

        <include layout="@layout/common_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y10"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/missionDisComAct_tv_carModel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xxx"
                android:textColor="#2c2c2c"
                android:textSize="15sp"/>

        </LinearLayout>

    </LinearLayout>

    <!--电量续航-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y98"
        android:layout_marginTop="@dimen/y7"
        android:background="@android:color/white"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:paddingLeft="@dimen/x12"
        android:paddingRight="@dimen/x12">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/y10"
            android:layout_marginRight="@dimen/x79"
            android:drawableLeft="@mipmap/mission"
            android:drawablePadding="@dimen/x7"
            android:text="电量续航"
            android:textColor="#5c5c5c"
            android:textSize="14sp"/>

        <include layout="@layout/common_line"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y10"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/missionDisComAct_tv_powerAndCanRange"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/xxx"
                android:textColor="#2c2c2c"
                android:textSize="15sp"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>